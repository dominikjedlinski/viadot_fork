version: 1

sources:

  - sharepoint_dev:
      class: Sharepoint
      credentials:
        site: "<TENANT_NAME>.sharepoint.com"
        username: "<USERNAME>"
        password: "<PASSWORD>"

  - supermetrics:
      class: Supermetrics
      credentials:
        api_key: "<API_KEY>",
        user: "<USER>",
  
  - exchange_rates_dev:
      class: ExchangeRates
      credentials:
        api_key: "<API_KEY>"

  - redshift_dev:
      class: RedshiftSpectrum
      credentials:
        profile_name: "dev"
        region_name: "eu-west-1"
        credentials_secret: "my_aws_secret_with_redshift_creds"
        iam_role: "arn:aws:iam::12345678910:role/my_redshift_role"

  - s3_dev:
      class: S3
      credentials:
        profile_name: "dev"
        region_name: "eu-west-1"